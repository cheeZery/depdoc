{
    "name": "cheezery/depdoc",
    "description": "Helps you document dependencies of your project",
    "type": "project",
    "homepage": "https://github.com/cheeZery/depdoc",
    "license": "MIT",
    "authors": [
        {
            "name": "Carsten Dietzel",
            "email": "carsten.dietzel@googlemail.com"
        }
    ],
    "bin": [
        "bin/depdoc"
    ],
    "autoload": {
        "psr-4": {
            "DepDoc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DepDocTest\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "ext-json": "*",
        "ext-pcre": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "phpspec/prophecy": "^1.7"
    }
}
